[color]
	branch      = auto
	diff        = auto
	status      = auto
	ui          = auto
	interactive = auto

[color "branch"]
	current = yellow reverse
	local   = yellow
	remote  = green

[color "diff"]
	meta       = yellow bold
	frag       = magenta bold
	old        = red bold
	new        = green bold
	whitespace = white reverse

[color "status"]
	added     = yellow
	changed   = green
	untracked = cyan

[core]
	editor       = vim
	whitespace   = fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = ~/.gitignore_global
	pager        = less -FXRS -x2

[user]
	name = Zachary Danger Campbell
	email = zacharydangercampbell@gmail.com

[push]
	default = current

[rerere]
	enabled = true

[instaweb]
  local = true
  httpd = webrick

[github]
	user  = zacharydanger
	token = 991538a8bcfe605a85e9da9a8afd0c8ddc440954

[apply]
	whitespace = nowarn

[branch]
	autosetupmerge = true

[rebase]
	autosquash = true

[filter "media"]
	clean  = git-media-clean %f
	smudge = git-media-smudge %f

[filter "lfs"]
	clean    = git-lfs clean -- %f
	smudge   = git-lfs smudge -- %f
	required = true
	process  = git-lfs filter-process

[alias]
  fap  = fetch --all --prune
  fum  = fetch upstream master:master
  fuss = fetch upstream staging:staging
  brm  = branch --merged
  brrm = branch --remotes --merged
	recent = branch --sort=-committerdate --format=\"%(committerdate:relative)\t\t\t\t\t%(refname:short)\"
[pull]
	rebase = false
[credential]
	helper = osxkeychain
